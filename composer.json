{
  "name": "bys-press/wordpress-theme",
  "description": "BYS Press - Modern WordPress theme with Timber and Vite",
  "type": "project",
  "license": "0BSD",
  "require": {
    "timber/timber": "^2.3",
    "kucrut/vite-for-wp": "^0.12.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "mockery/mockery": "^1.6",
    "brain/monkey": "^2.6"
  },
  "autoload": {
    "psr-4": {
      "BYSPress\\": "inc/"
    },
    "classmap": ["components/"]
  },
  "autoload-dev": {
    "psr-4": {
      "BYSPress\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "test:coverage": "phpunit --coverage-html coverage"
  }
}
